//94. binary tree inorder traversal

class Solution {
public:
    vector<int> res;
    void abc(TreeNode* node){
        if (node == NULL) return;
        abc(node->left);
        res.push_back(node->val);
        abc(node->right);
    }
    
    vector<int> inorderTraversal(TreeNode* root) {
        abc(root);
        return res;
    }
};

